{
  "name": "trash-common",
  "version": "3.0.0",
  "description": "Common Module of ThrowTrash Productions",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run fix-extensions",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "fix-extensions": "find lib/esm -name \"*.js\" -type f -exec bash -c 'mv \"$1\" \"${1%.js}.mjs\"' - {} \\; && find lib/cjs -name \"*.mjs\" -type f -exec bash -c 'mv \"$1\" \"${1%.mjs}.js\"' - {} \\;"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "moment-timezone": "^0.5.43"
  }
}
